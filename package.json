{
	"name": "strongr",
	"version": "1.0.0",
	"description": "Application to calculate 1RM and workout schedules",
	"main": "index.js",
	"author": "John Tran",
	"license": "MIT",
	"engines": {
		"yarn": "1.*"
	},
	"scripts": {
		"dev": "npm-run-all -p dev:start dev:wds",
		"dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
		"dev:wds": "cross-env NODE_ENV=development webpack-dev-server  --progress --mode development --config webpack.config.dev.babel.js",
		"flow": "flow",
		"flow-typed": "rm -rf flow-typed && flow-typed install --overwrite || true",
		"lint": "eslint src webpack.config.development.babel.js --ext .js,.jsx",
		"lint-fix": "prettier --single-quote --write \"{,!(dist|flow-typed|lib)/**}/*.{js,jsx}\" && yarn lint --fix",
		"prod": "yarn prod:build && yarn prod:start",
		"prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress --mode production --config webpack.config.prod.babel.js",
		"prod:start": "heroku local",
		"prod:analyze": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=analyze webpack -p --progress --mode production --config webpack.config.analyze.babel.js",
		"reinstall": "rm -rf node_modules/ && yarn install",
		"test": "jest --coverage",
		"test:unit": "jest",
		"heroku-postbuild": "yarn prod:build"
	},
	"browserslist": [
		"> 1%"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-flow-react-proptypes": "^24.0.0",
		"babel-preset-env": "^1.5.2",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"coveralls": "^3.0.0",
		"cross-env": "^5.1.4",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-compat": "^2.2.0",
		"eslint-plugin-flowtype": "^2.46.3",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-react": "^7.7.0",
		"flow-bin": "^0.70.0",
		"jest": "^22.4.3",
		"nodemon": "^1.17.3",
		"npm-run-all": "^4.0.2",
		"postcss": "^6.0.21",
		"postcss-cssnext": "^3.1.0",
		"postcss-loader": "^2.1.4",
		"precss": "^2.0.0",
		"rimraf": "^2.6.1",
		"style-loader": "^0.21.0",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.6.0",
		"webpack-bundle-analyzer": "^2.11.1",
		"webpack-cli": "^2.0.15",
		"webpack-dev-server": "^3.1.3",
		"webpack-merge": "^4.1.2"
	},
	"dependencies": {
		"babel-polyfill": "^6.23.0",
		"bootstrap": "4.3.1",
		"compression": "^1.7.2",
		"express": "^4.16.3",
		"express-sslify": "^1.2.0",
		"localforage": "^1.7.1",
		"lodash": "^4.17.5",
		"react": "16.3.2",
		"react-addons-css-transition-group": "^15.6.0",
		"react-addons-transition-group": "^15.6.0",
		"react-dom": "16.3.2",
		"react-helmet": "^5.1.3",
		"react-hot-loader": "4.1.1",
		"react-router": "^4.1.2",
		"react-router-dom": "^4.1.2",
		"reactstrap": "5.0.0",
		"styled-components": "^3.2.6"
	}
}
