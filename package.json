{
	"name": "strongr",
	"version": "1.0.0",
	"description": "Application to calculate 1RM and workout schedules",
	"main": "index.js",
	"author": "John Tran",
	"license": "MIT",
	"engines": {
		"node": "8.*",
		"yarn": "1.*"
	},
	"scripts": {
		"dev": "npm-run-all -p dev:start dev:wds",
		"dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
		"dev:wds": "webpack-dev-server --progress",
		"flow": "flow",
		"flow-typed": "rm -rf flow-typed && flow-typed install --overwrite || true",
		"prod": "yarn prod:build && yarn prod:start",
		"prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
		"prod:start": "heroku local",
		"lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
		"lint-fix": "prettier --single-quote --write \"{,!(dist|flow-typed|lib)/**}/*.{js,jsx}\" && yarn lint --fix",
		"test": "jest --coverage",
		"test:unit": "jest",
		"heroku-postbuild": "yarn prod:build"
	},
	"browserslist": [
		"> 1%"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-eslint": "^8.0.1",
		"babel-jest": "^21.2.0",
		"babel-loader": "^7.0.0",
		"babel-plugin-flow-react-proptypes": "^6.1.0",
		"babel-preset-env": "^1.5.2",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"coveralls": "^3.0.0",
		"cross-env": "^5.0.1",
		"eslint": "^4.9.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-compat": "^2.0.1",
		"eslint-plugin-flowtype": "^2.35.0",
		"eslint-plugin-import": "^2.7.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-react": "^7.0.1",
		"flow-bin": "^0.57.3",
		"jest": "^21.2.1",
		"nodemon": "^1.11.0",
		"npm-run-all": "^4.0.2",
		"postcss": "^6.0.8",
		"postcss-cssnext": "^3.0.2",
		"postcss-loader": "^2.0.6",
		"precss": "^2.0.0",
		"rimraf": "^2.6.1",
		"style-loader": "^0.19.0",
		"webpack": "^3.4.1",
		"webpack-dev-server": "^2.6.1"
	},
	"dependencies": {
		"babel-polyfill": "^6.23.0",
		"bootstrap": "4.0.0-alpha.6",
		"compression": "^1.7.0",
		"express": "^4.15.3",
		"localforage": "^1.5.5",
		"lodash": "^4.17.4",
		"react": "16.0.0",
		"react-addons-css-transition-group": "^15.6.0",
		"react-addons-transition-group": "^15.6.0",
		"react-dom": "16.0.0",
		"react-helmet": "^5.1.3",
		"react-hot-loader": "next",
		"react-router": "^4.1.2",
		"react-router-dom": "^4.1.2",
		"reactstrap": "^4.8.0",
		"styled-components": "^2.1.0"
	}
}
